{
  "name": "averoajs",
  "version": "1.0.0",
  "description": "'laravel like' boiler plate for express",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server",
    "dev": "nodemon server",
    "test": "jest",
    "mail": "node app/Jobs/EmailSubscriberJob",
    "generate:token": "node app/Jobs/GenerateAppTokenJob",
    "migrate:make": "knex migrate:make --knexfile node_modules/@averoa/core/knexfile.js",
    "seed:make": "knex seed:make --knexfile node_modules/@averoa/core/knexfile.js",
    "migrate:up": "knex migrate:latest --knexfile node_modules/@averoa/core/knexfile.js",
    "migrate:down": "knex migrate:rollback --knexfile node_modules/@averoa/core/knexfile.js",
    "migrate:list": "knex migrate:list --knexfile node_modules/@averoa/core/knexfile.js",
    "seed:run": "knex seed:run --knexfile node_modules/@averoa/core/knexfile.js"
  },
  "author": {
    "name": "Budi Hermawan",
    "email": "budianonym@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "@averoa/core": "^1.0.12",
    "@averoa/helpers": "^1.0.1",
    "@averoa/jobs": "^1.0.1",
    "@averoa/models": "^1.0.1",
    "@averoa/providers": "^1.0.1",
    "@averoa/repositories": "^1.0.1",
    "@averoa/routes": "^1.0.8",
    "@averoa/utilities": "^1.0.7",
    "amqplib": "^0.10.0",
    "axios": "^0.27.2",
    "chalk": "^4.1.2",
    "commander": "^9.3.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "csurf": "^1.11.0",
    "dotenv": "^16.0.1",
    "edge.js": "^5.3.6",
    "express": "^4.18.1",
    "express-handlebars": "^6.0.6",
    "express-session": "^1.17.3",
    "glob": "^8.0.3",
    "helmet": "^5.1.0",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.1.0",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "nodemailer": "^6.7.5",
    "nodemon": "^2.0.16",
    "objection": "^3.0.1",
    "passport": "^0.6.0",
    "passport-google-oauth": "^2.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "validator": "^13.7.0",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.2.0",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^28.1.1"
  }
}
